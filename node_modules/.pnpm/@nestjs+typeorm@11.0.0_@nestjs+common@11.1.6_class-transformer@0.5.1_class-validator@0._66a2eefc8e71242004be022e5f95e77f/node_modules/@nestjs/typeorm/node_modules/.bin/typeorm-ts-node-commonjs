#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules/typeorm/node_modules:/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules:/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules/typeorm/node_modules:/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules:/Users/samuelcn/Documents/Project/creatoria/creatoria-saas-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.26_ioredis@5.7.0_pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_ts-node@10.9._9405f5f5092c2a1de0ebc84d6320922c/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
