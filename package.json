{
  "name": "@creatoria/cli",
  "version": "0.1.0",
  "description": "A powerful CLI for building enterprise-grade SaaS applications",
  "author": "Creatoria Team",
  "private": false,
  "license": "MIT",
  "bin": {
    "cra": "./bin/cra"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rn1024/creatoria-saas-cli.git"
  },
  "keywords": [
    "cli",
    "saas",
    "nestjs",
    "scaffold",
    "creatoria",
    "typescript",
    "enterprise",
    "modular"
  ],
  "homepage": "https://github.com/rn1024/creatoria-saas-cli#readme",
  "bugs": {
    "url": "https://github.com/rn1024/creatoria-saas-cli/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli": "ts-node src/main.ts",
    "module:install": "npm run cli module install",
    "module:list": "npm run cli module list",
    "db:migrate": "npm run cli db migrate",
    "db:seed": "npm run cli db seed",
    "docker:build": "docker build -t creatoria-cli .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:all": "jest --coverage --verbose",
    "test:coverage": "jest --coverage && npm run coverage:report",
    "coverage:report": "node scripts/coverage-report.js",
    "coverage:open": "open coverage/lcov-report/index.html",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/fs-extra": "^11.0.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.0",
    "cache-manager-redis-yet": "^5.1.5",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.1",
    "inquirer": "^12.9.4",
    "lru-cache": "^11.2.1",
    "nest-commander": "^3.12.0",
    "p-limit": "^7.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "simple-git": "^3.28.0",
    "swagger-ui-express": "^5.0.1",
    "ts-morph": "^26.0.0",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
